{
  "manifest_version": 2,

  "name": "Lumosity",
  "description": "Take a Lumosity brain and refocus your brain!",
  "version": "1.5.2",

  "permissions": [
    "alarms",
    "notifications",
    "background",
    "tabs",
    "https://staging-6.lumosity.com/api/oauth/token",
    "http://static.sl.lumosity.com/compiled/trainer/lumosity_bridge-5bfbfa77a06a9de722bd79b2528e59a9.swf",
    "http://static.sl.lumosity.com/*"
  ],

  "web_accessible_resources": [
    "http://static.sl.lumosity.com/compiled/trainer/lumosity_bridge-5bfbfa77a06a9de722bd79b2528e59a9.swf",
    "http://static.sl.lumosity.com/*"
  ],

  "chrome_url_overrides" : {
    "newtab": "html/background.html"
  },

  "sandbox": {
    "pages": [
      "html/game.html",
      "game.html"
    ]
  },

  "browser_action": {
    "default_icon": {
      "19": "brain_19x19.png",
      "38": "brain_76x76.png"
    },
    "default_popup": "html/popup.html"
  },

  "background": {
    "page": "html/real_background.html",
    "persistent": false
  }
}
